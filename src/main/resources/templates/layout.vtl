<!DOCTYPE html>
<html>
  <head>
    <title>Review editor#if ($stepNo) - step $stepNo #end</title>
    <style media="screen" type="text/css">
		#parse( $headStyle )
			
		#if($pageStyle)
		#parse($pageStyle)
		#end
	</style>
    <link rel='stylesheet' href='//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css'>
    #if ($scripts)
    <link rel="stylesheet" href="//code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
    <script src="//code.jquery.com/jquery-1.8.3.min.js" type="text/javascript"></script>
    <script src="//code.jquery.com/ui/1.12.1/jquery-ui.min.js" type="text/javascript"></script>
    #if ($stepNo.equals("1"))
    <link rel="stylesheet" href="aehlke-tag-it/css/jquery.tagit.css" type="text/css">
    <script src="aehlke-tag-it/js/tag-it.js" type="text/javascript" charset="utf-8"></script>
    #end
    #if ($stepNo.equals("2"))
    <link rel="stylesheet" href="slider.css" type="text/css">
    <script src="bootstrap-slider.js" type="text/javascript" charset="utf-8"></script>
    #end
    #foreach($script in $scripts)
    <script type="text/javascript" src="$script"></script>
    #end
    #end
    <script>
    	#parse( $headScript )
    </script>
    
  </head>
  <body>
    <div class="container">
    
      #parse( $head )
		
      #parse( $template )

    </div>
  </body>
</html>